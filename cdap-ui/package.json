    "q": "1.4.1",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "request": "2.69.0",
    "serve-favicon": "2.3.0",
    "sockjs": "0.3.15"
  },
  "engines": {
    "node": ">= 0.10.0"
  }
}
