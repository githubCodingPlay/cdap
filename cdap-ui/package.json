{
  "name": "cdap-ui",
  "version": "3.6.0-SNAPSHOT",
  "description": "Front-end for CDAP",
  "scripts": {
    "start": "node ./server.js",
    "prebuild": "npm install && ./node_modules/bower/bin/bower update --config.interactive=false",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "distribute": "node ./node_modules/gulp/bin/gulp.js distribute",
    "test": "node ./node_modules/karma/bin/karma start test/karma-conf.js",
    "test-single-run": "node ./node_modules/karma/bin/karma start test/karma-conf.js --no-auto-watch --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caskdata/cdap/cdap-ui"
  },
  "devDependencies": {
    "autoprefixer": "6.3.2",
    "babel-preset-es2015": "6.3.13",
    "bower": "1.7.2",
    "del": "2.2.0",
    "es6-promise": "3.0.2",
    "gulp": "3.9.0",
    "gulp-header": "1.8.2",
    "gulp-angular-templatecache": "1.8.0",
    "gulp-babel": "6.1.1",
    "gulp-concat": "2.6.0",
    "gulp-cssnano": "2.1.0",
    "gulp-htmlmin": "1.3.0",
    "gulp-if": "2.0.0",
    "gulp-jshint": "2.0.0",
    "gulp-less": "3.0.5",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.2.0",
    "gulp-ng-annotate": "1.1.0",
    "gulp-plumber": "1.0.1",
    "gulp-postcss": "6.1.0",
    "gulp-replace": "0.5.4",
    "gulp-rev": "7.0.0",
    "gulp-stylelint": "2.0.2",
    "gulp-uglify": "1.5.1",
    "gulp-util": "3.0.7",
    "gulp-wrapper": "1.0.0",
    "jshint": "2.9.2",
    "karma": "0.12.31",
    "karma-chrome-launcher": "0.1.7",
    "karma-jasmine": "0.1.6",
    "karma-mocha-reporter": "1.1.1",
    "main-bower-files": "2.11.1",
    "merge-stream": "1.0.0"
  },
  "dependencies": {
    "body-parser": "1.14.2",
